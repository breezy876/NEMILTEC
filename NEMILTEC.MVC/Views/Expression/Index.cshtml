


@using Newtonsoft.Json
@model NEMILTEC.MVC.Models.PageModel

@{

    var modelJSON = Newtonsoft.Json.JsonConvert.SerializeObject(Model, new JsonSerializerSettings
    {
        TypeNameHandling = TypeNameHandling.Objects
    });
}

