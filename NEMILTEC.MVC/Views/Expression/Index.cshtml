            <h4>{{model.Test.Properties['Test1'].Value}}</h4>*@
        <div class="container">
            @*@{
                Html.RenderPartial("_PageContainerPartial", Model);
            }*@

            <span class="ui label visible expr">
                <span class="ui label visible expr">
                    <span class="ui label transition visible expr">
